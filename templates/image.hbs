import { useStaticQuery, graphql } from "gatsby"
import Img from "gatsby-image"
import * as React from "react"
import { ExtraImageProps } from "{{upPathImage pagePath}}/types/shared"

const alt = {
  name: "TODO",
}

export const {{getComponentImageName pagePath}}: React.FunctionComponent<ExtraImageProps & { image: keyof typeof alt }> = ({
  className = "",
  image,
  fluidObject = {}
  imgStyle = {},
}) => {
  const data = useStaticQuery(graphql`
    query {
      name: file(relativePath: { eq: "{{pagePath}}/name.jpg" }) {
        childImageSharp {
          fluid(maxWidth: 1200, quality: 70) {
            ...GatsbyImageSharpFluid_withWebp
          }
        }
      }
    }
  `)
  return (
    <Img
      fluid={{ ...data[image].childImageSharp.fluid, ...fluidObject }}
      alt={alt[image]}
      className={className}
      imgStyle={imgStyle}
    />
}
